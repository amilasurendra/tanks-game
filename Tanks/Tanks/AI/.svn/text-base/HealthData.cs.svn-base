using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Tanks.AI
{
    public class HealthData
    {
        public int x;
        public int y;
        public int span;
        public DateTime addedTime;
        public int remainingTime;

        public HealthData(int x, int y, int span)
        {
            this.x = x;
            this.y = y;
            this.span = span;
            this.addedTime = DateTime.Now;
        }
    }
}
